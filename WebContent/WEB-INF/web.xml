<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">

	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>com.iu.s1.HomeController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/index.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Member</servlet-name>
		<servlet-class>com.iu.s1.member.MemberController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Member</servlet-name>
		<url-pattern>/member/*</url-pattern><!-- 멤버로 시작하는 모든 애들,, 계속 새로 만들 순 없으니까 
			ㅇㅇ -->
	</servlet-mapping>
	<servlet>
		<servlet-name>BankBook</servlet-name>
		<servlet-class>com.iu.s1.bankbook.BankBookController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BankBook</servlet-name>
		<url-pattern>/bankbook/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Trade</servlet-name>
		<servlet-class>com.iu.s1.trade.TradeController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Trade</servlet-name>
		<url-pattern>/trade/*</url-pattern>
	</servlet-mapping>
	
	

</web-app>